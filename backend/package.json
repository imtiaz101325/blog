{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rimraf build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "migration": "knex --knexfile build/knexfile.js migrate:latest",
    "migration:list": "knex --knexfile build/knexfile.js migrate:list",
    "migration:rollback": "knex --knexfile build/knexfile.js migrate:rollback",
    "start": "npm run clean && npm run build && nodemon --watch build ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "morgan": "~1.9.1",
    "pg": "^8.0.3",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.5",
    "nodemon": "^2.0.3",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
